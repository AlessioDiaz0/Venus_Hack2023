{
  "manifest_version": 3,
  "name": "Extension2",
  "version": "0.1.0",
  "description": "My Chrome Extension",
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_title": "Extension2",
    "default_popup": "popup.html"
  },
  "permissions": [
        "tabs",
        "storage",
        "identity",
        "identity.email",
        "activeTab",
        "scripting"
  ],

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_idle",
      "js": [
        "contentScript.js"
      ]
    }
  ],
    "oauth2": {
        "client_id": "121608638258-dsb5tcb2ouo7oeqlcvefmcv9vdao3djh.apps.googleusercontent.com",
        "scopes": [
            "profile email",
            "https://www.googleapis.com/auth/contacts",
            "https://www.googleapis.com/auth/userinfo.profile"
        ]
    }
}